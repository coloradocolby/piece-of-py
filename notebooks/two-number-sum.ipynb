   "source": [
    "def two_number_sum(arr, target):\n",
    "    for i in range(0, len(arr) - 1):\n",
    "        for j in range(i+1, len(arr)):\n",
    "            if arr[i] + arr[j] == target:\n",
    "                return [ arr[i], arr[j]]\n",
    "    return []"
   ]
  },
